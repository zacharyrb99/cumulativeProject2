{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "salary": {
      "type": "integer",
      "minimum": 0
    },
    "equity": {
      "type": "number",
      "maximum": 1.0
    },
    "company_handle": {
      "type": "string",
      "maxLength": 25
    }
  },
  "additionalProperties": false,
  "required": [
    "title"
  ]
}